.right-bar {
	.form-inline {
		margin: 5px 0;
	}
	.middle-condition .form-group {
		width: 100px;
	}
	.middle-condition .form-control {
		width: 100%;
	}
	.nav-select .selected {
	    border-bottom: none;
	}
	// table.agent-list {
	// 	tr th {
