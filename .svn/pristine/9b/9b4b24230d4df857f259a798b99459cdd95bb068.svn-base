.right-bar {
	.nav-select .selected {
		border-bottom: none;
	}
	.middle-condition {
		margin: 5px 0;
		.form-group {
		    width: 130px;
		}
		.form-control {
		    width: 100%;
		}
		.inline-btn {
			width: auto;
		}
	}
	tr th,tr td {
		text-align: center;
	}
	.table {
		margin-bottom: 5px;
	}
	.img-info {
		padding: 15px 5px 5px 5px;
		width: 450px;
		border: 1px dashed #FF0000;
		overflow-y: hidden;
		.item {
			float: left;
			margin-right: 15px;
			img {
				display: block;
				margin-top: 5px;
				width: 200px;
				height: 100px;
			}
		}
	}
}
.check-process {
	span {
		font-weight: bold;
	}
}


